{
    extends: "eslint:recommended",
    env: {
        browser: true
    },
    globals: {
        jQuery: true,
        $: true
    },
    rules: {
        no-console: 1,
        no-alert: 1,
        quotes: [2, "single"],
        consistent-return: 1,
        default-case: 1,
        eqeqeq: 1,
        camelcase: [2, {properties: "always"}],
        no-unused-vars: 1
    }
}
